type Achievement implements ParentWithGoal{
    id: UUID
    name : String!
    imageUrl: String!
    description: String!
    courseId: UUID!
    userId: UUID
    completed: Boolean!
    requiredCount: Int
    completedCount: Int
    trackingStartTime: DateTime
    trackingEndTime: DateTime
}

interface ParentWithGoal {
    courseId: UUID!
    userId: UUID
}